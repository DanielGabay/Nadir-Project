{
  "name": "nadir",
  "version": "1.0.0",
  "description": "Nadir Project",
  "main": "index.js",
  "scripts": {
    "start": "electron index.js",
    "package": "",
    "build": "electron-packager . Nadir",
    "package-mac": "electron-packager . Nadir_Project --overwrite --asar=true --platform=darwin --arch=x64 --icon=src/icons/mac/icon.icns --prune=true --out=release-builds_Nadir --version-string.CompanyName=CE --version-string.FileDescription=CE --version-string.ProductName=\"Nadir project\"",
    "package-win": "electron-packager . Nadir_Project --overwrite --asar=true --platform=win32 --arch=ia32 --icon=src/icons/win/icon.ico --prune=true --out=release-builds_Nadir --version-string.CompanyName=CE --version-string.FileDescription=CE --version-string.ProductName=\"Nadir project\"",
    "package-linux": "electron-packager . Nadir_Project --overwrite --asar=true --platform=linux --arch=x64 --icon=src/icons/linux/icon.png --prune=true --out=release-builds_Nadir --version-string.CompanyName=CE --version-string.FileDescription=CE --version-string.ProductName=\"Nadir project\""
  },
  "author": "Daniel Gabay, Esty Kolin, Sarai Zarbib, Shachar Israeli, Guy Udi",
  "license": "MIT",
  "devDependencies": {
    "electron": "^5.0.0",
    "electron-packager": "^13.1.1"
  },
  "dependencies": {
    "@chenfengyuan/datepicker": "^1.0.7"
  }
}
